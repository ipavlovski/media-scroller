{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "module": "commonjs"
  },
  "ts-node": {
    "esm": true,
    "compilerOptions": {
      "target": "esnext",
      "module": "CommonJS",
      "esModuleInterop": true,
      "types": ["node"],
      "moduleResolution": "Node",
      "lib": ["esnext", "DOM"],
      "strict": true
    },
    "ignoreDiagnostics": [2393, 2300],
    "exclude": ["node_modules", "scratch.ts"]
  }
}
